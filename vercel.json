{
    "version":2,
    "builds":[{
        "src":"demoproject/wsgi.py",
        "use":"@vercel/python",
        "config":{
            "maxlamdasize":"15mb",
            "runtime":"python3.9"
        }
    },{
        "src":"builds_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "distdir":"staticfiles_build"
        }
    }
],
"routes":
    [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },{
            "src":"/(.*)",
            "dest":"demoproject/wsgi.py"
        }
    ]
}